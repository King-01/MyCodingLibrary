int binary_search(int lo, int hi)
{
	while(lo < hi)
	{
		int mid = lo + hi;
		mid /= 2;
		dbg(mid);
		if(check(mid))
		{
			hi = mid;
		}
		else lo = mid + 1;
	}
	return hi;
}
]]></content>
