ll powe(ll x, ll y) 
{ 
    ll res = 1;
  
    x = x % hell;

    while (y > 0) 
    { 
        if (y & 1) 
            res = (res*x) % hell; 
  
        y = y>>1; 
        x = (x*x) % hell; 
    } 
    return res; 
} ]]></content>
